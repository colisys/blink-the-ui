{
  "name": "@blink-the-ui/theme",
  "version": "0.0.1-alpha.21",
  "description": "theme for blink-the-ui",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && mkdir dist",
    "dist": "pnpm clean && pnpm build && pnpm types",
    "types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop src/index.ts"
  },
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts",
        "require": "./src/index.ts"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.umd.cjs"
      }
    },
    "./style.css": {
      "development": "./src/index.less",
      "default": "./dist/index.css"
    },
    "./index.less": "./src/index.less"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "blink-the-ui",
    "vue3",
    "theme"
  ],
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0",
    "vite": "^7.1.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
