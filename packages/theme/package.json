{
  "name": "@blink-the-ui/theme",
  "version": "0.0.1-alpha.20",
  "description": "theme for blink-the-ui",
  "main": "dist/index.umd.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "vue-tsc -b && vite build",
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop --module preserve --moduleResolution bundler src/index.ts",
    "dist": "pnpm clean && mkdir dist && pnpm build && pnpm types"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    },
    "./style.css": "./dist/index.css",
    "./index.less": "./src/index.less"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "theme"
  ],
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0",
    "vite": "^7.1.9"
  },
  "publishConfig": {
    "access": "public"
  }
}
