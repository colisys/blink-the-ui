{
  "name": "@blink-the-ui/helper",
  "version": "0.0.1-alpha.13",
  "description": "helper for blink-the-ui",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "private": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop src/index.ts",
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop --module preserve --moduleResolution bundler src/index.ts",
    "dist": "pnpm clean && pnpm build && pnpm types"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "helper"
  ],
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0"
  }
}
