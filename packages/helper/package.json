{
  "name": "@blink-the-ui/helper",
  "version": "0.0.1-alpha.19",
  "description": "helper for blink-the-ui",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf dist",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist src/index.ts",
    "dist": "pnpm clean && mkdir dist && pnpm build && pnpm types"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "helper"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist"
  ],
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {
    "vue-i18n": "^9.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.9"
  }
}
