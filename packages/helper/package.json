{
  "name": "@blink-the-ui/helper",
  "private": false,
  "version": "0.0.1-alpha.22",
  "description": "helper for blink-the-ui",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && mkdir dist",
    "dist": "pnpm clean && pnpm build && pnpm types",
    "types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop src/index.ts"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "helper"
  ],
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "development": {
        "types": "./src/index.ts",
        "import": "./src/index.ts",
        "require": "./src/index.ts"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.umd.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {
    "vue-i18n": "^9.0.0"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.0",
    "@types/node": "^24.9.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "vite": "^7.1.9"
  }
}
