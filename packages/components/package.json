{
  "name": "@blink-the-ui/components",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && mkdir dist",
    "dist": "pnpm clean && pnpm build && pnpm types",
    "types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop src/index.ts"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/components/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@blink-the-ui/animations": "workspace:animations",
    "@blink-the-ui/directives": "workspace:directives",
    "@blink-the-ui/i18n": "workspace:i18n",
    "@blink-the-ui/helper": "workspace:helper",
    "@blink-the-ui/overlays": "workspace:overlays",
    "@blink-the-ui/layout": "workspace:layout",
    "@blink-the-ui/theme": "workspace:theme",
    "vue": "^3.5.22",
    "vue-i18n": "^9.0.0"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "components"
  ],
  "author": "colisys <colisys@duck.com>",
  "devDependencies": {
    "@types/less": "^3.0.8",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "@vue/tsconfig": "^0.8.1",
    "less": "^4.4.2",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vue-tsc": "^3.1.0"
  }
}
