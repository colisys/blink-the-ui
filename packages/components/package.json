{
  "name": "@blink-the-ui/components",
  "private": false,
  "version": "0.0.1-alpha.20",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf dist && mkdir dist",
    "dist": "pnpm clean && pnpm build && pnpm types",
    "types": "vue-tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop --module preserve --moduleResolution bundler src/index.ts"
  },
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    },
    "./style.css": "./dist/index.css",
    "./index.less": "./src/index.less"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@blink-the-ui/animations": "workspace:*",
    "@blink-the-ui/directives": "workspace:*",
    "@blink-the-ui/helper": "workspace:*",
    "@blink-the-ui/i18n": "workspace:*",
    "@blink-the-ui/layout": "workspace:*",
    "@blink-the-ui/theme": "workspace:*",
    "async-validator": "^4.2.5",
    "vue": "^3.5.22",
    "vue-i18n": "^9.0.0"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "components"
  ],
  "author": "colisys <colisys@duck.com>",
  "devDependencies": {
    "@types/less": "^3.0.8",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/babel-plugin-jsx": "^2.0.1",
    "@vue/tsconfig": "^0.8.1",
    "less": "^4.4.2",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.9",
    "vue-tsc": "^3.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
