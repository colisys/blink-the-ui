{
  "name": "@blink-the-ui/i18n",
  "version": "0.0.1-alpha.14",
  "description": "i18n support for blink-the-ui",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop src/index.ts",
    "types": "tsc --declaration --emitDeclarationOnly --outDir dist --jsx preserve --resolveJsonModule --esModuleInterop --module preserve --moduleResolution bundler src/index.ts",
    "dist": "pnpm clean && pnpm build && pnpm types"
  },
  "keywords": [
    "blink-the-ui",
    "vue3",
    "i18n",
    "internationalization"
  ],
  "author": "colisys <colisys@duck.com>",
  "license": "MIT",
  "dependencies": {
    "vue-i18n": "^9.0.0"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@vue/tsconfig": "^0.8.1",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vue": "^3.0.0",
    "vue-tsc": "^3.1.0",
    "@types/node": "^24.9.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
